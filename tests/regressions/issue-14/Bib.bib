@article{lampropoulosAugmentedRealityGamification2022,
  title = {Augmented {{Reality}} and {{Gamification}} in {{Education}}: {{A Systematic Literature Review}} of {{Research}}, {{Applications}}, and {{Empirical Studies}}},
