@article{ahlersForeignLanguageTandem2022,
  title = {Foreign {{Language Tandem Learning}} in {{Social VR}}: {{Conception}}, {{Implementation}} and {{Evaluation}} of the {{Game-Based Application}} {{{\emph{Hololingo}}}}{\emph{!}}},
  shorttitle = {Foreign {{Language Tandem Learning}} in {{Social VR}}},
  author = {Ahlers, Timo and Bumann, Cassandra and K{\"o}lle, Ralph and Lazovi{\'c}, Milica},
