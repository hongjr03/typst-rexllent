@article{johnson-glenbergEffectsEmbodiedLearning2016,
  title = {Effects of {{Embodied Learning}} and {{Digital Platform}} on the {{Retention}} of {{Physics Content}}: {{Centripetal Force}}.},
  author = {{Johnson-Glenberg}, Colleen; Birchfield, David; Savio-Ramos, Caroline, Mina C.; Megowan-Romanowicz},
  year = {2016},
