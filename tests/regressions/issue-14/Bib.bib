@article{poradaResearchAgendaNonvascular2023,
  title = {A Research Agenda for Nonvascular Photoautotrophs under Climate Change},
  author = {Porada, Philipp and Bader, Maaike Y. and Berdugo, Monica B. and Colesie, Claudia and Ellis, Christopher J. and Giordani, Paolo and Herzschuh, Ulrike and Ma, Yunyao and Launiainen, Samuli and Nascimbene, Juri and Petersen, Imke and Raggio Qu{\'i}lez, Jos{\'e} and {Rodr{\'i}guez-Caballero}, Emilio and Rousk, Kathrin and Sancho, Leopoldo G. and Scheidegger, Christoph and Seitz, Steffen and Van Stan II, John T. and Veste, Maik and Weber, Bettina and Weston, David J.},
  year = {2023},
