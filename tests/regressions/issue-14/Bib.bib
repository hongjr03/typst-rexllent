@article{ohkiNitrogenReductionFe2022,
  title = {Nitrogen Reduction by the {{Fe}} Sites of Synthetic [{{Mo3S4Fe}}] Cubes},
  author = {Ohki, Yasuhiro and Munakata, Kenichiro and Matsuoka, Yuto and Hara, Ryota and Kachi, Mami and Uchida, Keisuke and Tada, Mizuki and Cramer, Roger E. and Sameera, W. M. C. and Takayama, Tsutomu and Sakai, Yoichi and Kuriyama, Shogo and Nishibayashi, Yoshiaki and Tanifuji, Kazuki},
