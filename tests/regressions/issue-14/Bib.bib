@inproceedings{szaboNotionImmersionVirtual2020,
  title = {The Notion of Immersion in Virtual Reality Literature and Related Sources},
  booktitle = {2020 11th {{IEEE International Conference}} on {{Cognitive Infocommunications}} ({{CogInfoCom}})},
