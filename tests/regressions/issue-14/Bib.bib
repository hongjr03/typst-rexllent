@article{kleijnAgriEnvironmentSchemesNot2001,
  title = {Agri-{{Environment Schemes Do Not Effectively Protect Biodiversity}} in {{Dutch Agricultural Landscapes}}},
  author = {Kleijn, David and Berendse, Frank and Smit, Ruben and Gilissen, Niels},
  year = {2001},
