@article{boussardAdaptiveBehaviourLearning2021,
  title = {Adaptive Behaviour and Learning in Slime Moulds: The Role of Oscillations},
  shorttitle = {Adaptive Behaviour and Learning in Slime Moulds},
  author = {Boussard, Aur{\`e}le and Fessel, Adrian and Oettmeier, Christina and Briard, L{\'e}a and D{\"o}bereiner, Hans-G{\"u}nther and Dussutour, Audrey},
  year = {2021},
  month = mar,
