@article{remolarLearningHistoryUsing2021,
  title = {Learning {{History Using Virtual}} and {{Augmented Reality}}},
  author = {Remolar, Inmaculada and Rebollo, Cristina and {Fern{\'a}ndez-Moyano}, Jon A.},
