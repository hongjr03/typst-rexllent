@article{fariasBiologicalN2OFixation2013,
  title = {Biological {{N2O Fixation}} in the {{Eastern South Pacific Ocean}} and {{Marine Cyanobacterial Cultures}}},
  author = {Far{\'i}as, Laura and Fa{\'u}ndez, Juan and Fern{\'a}ndez, Camila and Cornejo, Marcela and Sanhueza, Sandra and Carrasco, Cristina},
  year = {2013},
