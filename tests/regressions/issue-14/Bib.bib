@article{luoIntegratedModelingAgricultural2022,
  title = {Integrated {{Modeling}} of {{U}}.{{S}}. {{Agricultural Soil Emissions}} of {{Reactive Nitrogen}} and {{Associated Impacts}} on {{Air Pollution}}, {{Health}}, and {{Climate}}},
  author = {Luo, Lina and Ran, Limei and Rasool, Quazi Z. and Cohan, Daniel S.},
