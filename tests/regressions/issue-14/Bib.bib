@article{davisExtrinsicIntrinsicMotivation1992,
  title = {Extrinsic and {{Intrinsic Motivation}} to {{Use Computers}} in the {{Workplace}}{\textsuperscript{1}}},
  author = {Davis, Fred D. and Bagozzi, Richard P. and Warshaw, Paul R.},
  year = {1992},
